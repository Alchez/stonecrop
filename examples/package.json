{
	"name": "@stonecrop/examples",
	"version": "0.2.6",
	"description": "Example App(s) for the Stonecrop framework",
	"private": true,
	"type": "module",
	"license": "MIT",
	"main": "index.js",
	"files": [
		"aform/*",
		"docbuilder/*"
	],
	"scripts": {
		"build": "",
		"dev:aform": "cd aform/ && histoire dev",
		"dev:atable": "cd atable/ && histoire dev",
		"dev:beam": "cd beam/ && histoire dev",
		"dev:builder": "vite serve docbuilder/ --config docbuilder/vite.config.ts",
		"dev:code_editor": "cd code_editor/ && histoire dev",
		"dev:desktop": "vite dev desktop/ --config desktop/vite.config.ts",
		"dev:graphql_client": "vite dev graphql_client/ --config graphql_client/vite.config.ts",
		"dev:node_editor": "cd node_editor/ && histoire dev"
	},
	"dependencies": {
		"@stonecrop/aform": "workspace:*",
		"@stonecrop/atable": "workspace:*",
		"@stonecrop/beam": "workspace:*",
		"@stonecrop/code-editor": "workspace:*",
		"@stonecrop/desktop": "workspace:*",
		"@stonecrop/graphql-client": "workspace:*",
		"@stonecrop/node-editor": "workspace:*",
		"@stonecrop/stonecrop": "workspace:*",
		"@stonecrop/themes": "workspace:*",
		"@miragejs/graphql": "^0.1.13",
		"miragejs": "^0.1.48",
		"pinia": "^3.0.1",
		"vite": "^5.4.14",
		"vue": "^3.5.13",
		"vue-toast-notification": "^3.1.3",
		"xstate": "^4.38.3"
	},
	"devDependencies": {
		"@histoire/plugin-vue": "^0.17.17",
		"@monaco-editor/loader": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.14.1",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vue-flow/core": "^1.42.1",
		"cypress": "^14.0.3",
		"eslint": "^8.40.0",
		"eslint-plugin-vue": "^9.11.1",
		"histoire": "^0.17.17",
		"immutable": "^5.0.3",
		"monaco-editor": "^0.52.2",
		"portal-vue": "^3.0.0",
		"typescript": "~5.6.3",
		"vue-router": "^4.5.0"
	},
	"engines": {
		"node": ">=20.11.0"
	}
}
