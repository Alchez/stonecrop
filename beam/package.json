{
	"name": "@stonecrop/beam",
	"version": "0.4.6",
	"license": "MIT",
	"type": "module",
	"author": {
		"name": "Tyler Matteson",
		"email": "tyler@agritheory.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/agritheory/stonecrop",
		"directory": "beam"
	},
	"bugs": {
		"url": "https://github.com/agritheory/stonecrop/issues"
	},
	"exports": {
		".": {
			"import": "./dist/beam.js",
			"require": "./dist/beam.umd.cjs"
		},
		"./styles": "./dist/assets/index.css"
	},
	"main": "dist/beam.js",
	"module": "dist/beam.js",
	"files": [
		"dist/*",
		"src/*"
	],
	"scripts": {
		"prepublish": "heft build && vite build && rushx docs",
		"build": "heft build && vite build && rushx docs",
		"dev": "vite",
		"docs": "api-documenter markdown -i temp -o ../docs/beam",
		"lint": "eslint . --ext .ts,.vue",
		"preview": "vite preview",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui"
	},
	"dependencies": {
		"@vueuse/core": "^12.7.0",
		"mqtt": "^5.10.3",
		"onscan.js": "^1.5.2",
		"vue": "^3.5.13"
	},
	"devDependencies": {
		"@microsoft/api-documenter": "^7.26.10",
		"@rushstack/heft": "^0.68.18",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.14.1",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vitest/coverage-istanbul": "^3.0.6",
		"@vitest/ui": "^3.0.6",
		"@vue/test-utils": "^2.4.6",
		"cypress": "^14.0.3",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-vue": "^9.11.1",
		"sass": "^1.75.0",
		"stonecrop-rig": "workspace:*",
		"typescript": "~5.6.3",
		"vite-plugin-lib-inject-css": "^2.2.1",
		"vite": "^5.4.14",
		"vitest": "^3.0.6",
		"vue-router": "^4.5.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=20.11.0"
	}
}
